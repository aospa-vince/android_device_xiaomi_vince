<resources>

    <!-- Keycodes of gestures. If the gesture is not supported set it to 0. -->
    <integer name="config_doubleTapKeyCode">143</integer>

    <!-- Default settings for gestures
            0 - Disabled
            1 - Wake device
            2 - Toggle flashlight
            3 - Toggle airplane mode
            4 - Play/Pause music
            5 - Next music
            6 - Previous music
            7 - Open camera
            8 - Open dialer -->
    <integer name="config_doubleTapDefault">1</integer>

    <!-- Whether to intercept inputs on screen off. -->
    <bool name="config_enableKeyHandler">true</bool>

    <!-- Default list of files pinned by the Pinner Service -->
    <string-array translatable="false" name="config_defaultPinnerServiceFiles">
        <item>"/system/framework/framework.jar"</item>
        <item>"/system/framework/services.jar"</item>
        <item>"/system/framework/ext.jar"</item>
        <item>"/system/framework/QPerformance.jar"</item>
        <item>"/system/framework/QXPerformance.jar"</item>
        <item>"/system/framework/UxPerformance.jar"</item>
        <item>"/system/bin/surfaceflinger"</item>
        <item>"/apex/com.android.art/javalib/core-oj.jar"</item>
        <item>"/apex/com.android.art/javalib/core-libart.jar"</item>
        <item>"/system/system_ext/priv-app/ParanoidSystemUI/ParanoidSystemUI.apk"</item>
    </string-array>

    <!-- True if home app should be pinned via Pinner Service -->
    <bool name="config_pinnerHomeApp">false</bool>

    <!-- Should the pinner service pin the Camera application? -->
    <bool name="config_pinnerCameraApp">false</bool>

</resources>
